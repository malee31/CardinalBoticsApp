<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:layout_alignParentBottom="true"
	tools:context=".ui.forms.FormsFragment">

	<ScrollView
		android:layout_width="353dp"
		android:layout_height="694dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent">

		<LinearLayout
			android:id="@+id/resourcesList"
			android:layout_width="match_parent"
			android:layout_height="716dp"
			android:orientation="vertical"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintVertical_bias="1.0">

			<Button
				android:id="@+id/button1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_teamweb"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="Team Website"
				android:textSize="18sp" />


			<Button
				android:id="@+id/button2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_google_drive"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="Team Google drive"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_roster"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="roster"
				android:textSize="18sp" />


			<Button
				android:id="@+id/button5"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:autoLink="web"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_calendar"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="Google calendar"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button6"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_form"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="Form Mastersheet"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button7"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:autoLink="web"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_photos"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="google photos"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button8"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:autoLink="web"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_youtube"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="youtube channel"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:autoLink="web"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_handbook"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="handbooks"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:autoLink="web"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_traindoc"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="training document"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:autoLink="web"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_ircgamemanual"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="IRC Game Manual"
				android:textSize="18sp" />

			<Button
				android:id="@+id/button4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:autoLink="web"
				android:backgroundTint="#A17D1120"
				android:drawableRight="@drawable/ic_resource_grant"
				android:fontFamily="@font/roboto_bold"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="link"
				android:text="Grant Mastersheet"
				android:textSize="18sp" />

			<ScrollView
				android:layout_width="match_parent"
				android:layout_height="match_parent">

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical" />
			</ScrollView>

		</LinearLayout>
	</ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>